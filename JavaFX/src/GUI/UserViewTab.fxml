<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="800.0" stylesheets="@UsertabView.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.UserViewTabController">
   <children>
      <HBox fx:id="hBox" layoutY="-1.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="-1.0">
         <children>
            <TableView fx:id="tableView" prefHeight="400.0" prefWidth="320.0">
               <columns>
                  <TableColumn fx:id="symbol" prefWidth="95.0" text="Symbol" />
                  <TableColumn fx:id="quantity" prefWidth="132.0" text="Quantity" />
                  <TableColumn fx:id="price" prefWidth="89.0" text="Price" />
               </columns>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TableView>
            <Separator orientation="VERTICAL" prefHeight="400.0" prefWidth="21.0" style="-fx-background-color: #CFD8DC;" />
            <VBox fx:id="vBox" prefHeight="391.0" prefWidth="461.0" style="-fx-border-color: #212121;">
               <children>
                  <Label fx:id="username" alignment="CENTER" prefHeight="121.0" prefWidth="467.0" text="username" textAlignment="CENTER">
                     <font>
                        <Font size="32.0" />
                     </font>
                     <graphic>
                        <ImageView fx:id="userImg" fitHeight="80.0" fitWidth="82.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../Resources/user.png" />
                           </image>
                           <viewport>
                              <Rectangle2D />
                           </viewport>
                        </ImageView>
                     </graphic>
                  </Label>
                  <Label fx:id="holdingsValue" alignment="CENTER" prefHeight="121.0" prefWidth="463.0" text="Value">
                     <font>
                        <Font size="32.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="80.0" fitWidth="93.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../Resources/holdings.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Label>
                  <Button fx:id="tradeButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="100.0" prefWidth="80.0" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
                     <VBox.margin>
                        <Insets left="150.0" right="100.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="23.0" />
                     </font>
                     <graphic>
                        <ImageView fx:id="tradeImg" fitHeight="110.0" fitWidth="122.0" pickOnBounds="true">
                           <image>
                              <Image url="@../Resources/exchange.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
</AnchorPane>
