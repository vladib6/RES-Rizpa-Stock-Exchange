            <ImageView fx:id="lockImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Resources/pngegg.png" />
               </image>
            </ImageView>
         </graphic>
         <rotationAxis>
            <Point3D z="5.0" />
         </rotationAxis>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </Button>
   </center>
   <bottom>
      <Label fx:id="errMessage" alignment="CENTER" prefHeight="42.0" prefWidth="618.0" style="-fx-background-color: #CFD8DC ; -fx-font-size: 16; " text="Error message label" textAlignment="CENTER" textFill="#d70d0d" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
