<BorderPane fx:id="borderPane" prefHeight="452.0" prefWidth="616.0" stylesheets="@mainscreenstyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.mainscreencontroller">
